{
  "name": "okr-frontend",
  "version": "1.0.0",
  "scripts": {
    "prestart": "ts-appversion",
    "prebuild": "ts-appversion",
    "pretest": "ts-appversion",
    "start": "ng serve --proxy-config=proxy.conf.json",
    "start-dev": "npm run start -- --configuration local",
    "start-demo": "npm run start -- --configuration demo",
    "start-prod": "npm run start -- --configuration production",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "ng build",
    "build-prod": "npm run build -- --output-hashing=all --configuration production",
    "build-demo": "npm run build -- --output-hashing=all --configuration demo",
    "build-docker": "npm run build -- --output-hashing=all --configuration production --configuration docker --base-href=/",
    "lint": "ng lint",
    "stats": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.2",
    "@angular/cdk": "~14.2.2",
    "@angular/common": "~14.2.2",
    "@angular/compiler": "~14.2.2",
    "@angular/core": "~14.2.2",
    "@angular/flex-layout": "~14.0.0-beta.40",
    "@angular/forms": "~14.2.2",
    "@angular/localize": "~14.2.2",
    "@angular/material": "~14.2.2",
    "@angular/material-moment-adapter": "~14.2.2",
    "@angular/platform-browser": "~14.2.2",
    "@angular/platform-browser-dynamic": "~14.2.2",
    "@angular/platform-server": "~14.2.2",
    "@angular/router": "~14.2.2",
    "@ngx-translate/core": "~13.0.0",
    "@ngx-translate/http-loader": "~6.0.0",
    "@saithodev/ts-appversion": "~2.2.0",
    "@stomp/ng2-stompjs": "~8.0.0",
    "@stomp/rx-stomp": "~1.1.4",
    "@types/file-saver": "~2.0.3",
    "angular-oauth2-oidc": "~12.1.0",
    "angular-oauth2-oidc-jwks": "~12.0.0",
    "angular-resize-event": "~3.2.0",
    "class-transformer": "~0.4.0",
    "core-js": "~3.20.3",
    "file-saver": "~2.0.5",
    "linq4js": "~2.2.3",
    "moment": "~2.29.4",
    "ngx-avatars": "~1.4.0",
    "ngx-logger": "~4.3.3",
    "ngx-mat-select-search": "~4.0.0",
    "ngx-material-file-input": "~4.0.0",
    "ngx-papaparse": "~5.0.0",
    "ngx-wow": "~2.0.1",
    "raw-loader": "~4.0.2",
    "rxjs": "~6.6.7",
    "sass": "~1.54.4",
    "tslib": "~2.4.0",
    "wowjs": "~1.1.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.3",
    "@angular-eslint/builder": "~14.1.1",
    "@angular-eslint/eslint-plugin": "~14.1.1",
    "@angular-eslint/eslint-plugin-template": "~14.1.1",
    "@angular-eslint/schematics": "~14.1.1",
    "@angular-eslint/template-parser": "~14.1.1",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "~14.2.2",
    "@angular/language-service": "~14.2.2",
    "@types/jasmine": "~3.10.1",
    "@types/jest": "^29.4.0",
    "@types/node": "~16.11.6",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "~7.32.0",
    "eslint-plugin-jsdoc": "~37.0.3",
    "eslint-plugin-prefer-arrow": "~1.2.3",
