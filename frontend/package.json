{
  "name": "okrfront-end",
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/test.*",
      "<rootDir>/src/environments/environment.*"
    ]
  },
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "ts-appversion",
    "prebuild": "ts-appversion",
    "start": "ng serve --proxy-config=proxy.conf.json",
    "start-dev": "npm run start -- --configuration=local",
    "start_no_reload": "ng serve --proxy-config=proxy.conf.json --live-reload false",
    "start_en": "npm run start -- --configuration=en",
    "start-prod": "npm run start -- --prod",
    "build": "ng build",
    "build-en": "npm run build -- --prod --base-href=./ --configuration=en",
    "build-prod": "npm run build -- --output-hashing=all --prod --base-href=/app/",
    "build-prod-en": "npm run build -- --output-hashing=all --prod --base-href=/app/ --configuration=en",
    "build-prod-new": "npm run build -- --output-hashing=all --prod",
    "build-prod-en-new": "npm run build -- --output-hashing=all --prod --configuration=en",
    "build-docker": "npm run build -- --output-hashing=all --prod --configuration=docker --base-href=/",
    "test": "jest --verbose ./src/app",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stats": "webpack-bundle-analyzer dist/stats.json",
    "extract-i18n": "ng xi18n OKRFrontEnd --i18n-format xlf --output-path locale --i18n-locale de && ngx-extractor -i src/**/*.ts -f xlf -o src/locale/messages.xlf  && ng run OKRFrontEnd:xliffmerge"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular/animations": "^10.2.5",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.2.5",
    "@angular/localize": "~10.2.5",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/platform-server": "^10.2.5",
    "@angular/router": "^10.2.5",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "@saithodev/ts-appversion": "^2.1.3",
    "@stomp/ng2-stompjs": "^8.0.0",
    "@stomp/rx-stomp": "^1.1.2",
    "@types/file-saver": "^2.0.1",
    "angular-oauth2-oidc": "^8.0.4",
    "angular-resize-event": "^2.1.0",
    "class-transformer": "^0.2.3",
    "core-js": "^3.1.4",
    "file-saver": "^2.0.2",
    "linq4js": "^2.2.3",
    "ngx-avatar": "^4.1.0",
    "ngx-logger": "^4.0.7",
    "ngx-mat-select-search": "^3.3.0",
    "ngx-material-file-input": "^2.1.1",
    "ngx-papaparse": "^4.0.4",
    "node-sass": "^4.12.0",
    "rxjs": "^6.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "^10.2.3",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/jasmine": "~3.3.16",
    "@types/jasminewd2": "~2.0.6",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^25.0.0",
    "jest-preset-angular": "^8.0.0",
    "protractor": "~7.0.0",
    "rxjs-tslint-rules": "^4.30.1",
    "sw-precache": "^5.2.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8",
    "webpack-bundle-analyzer": "^3.4.1"
  }
}
