{
  "name": "okr-frontend",
  "version": "1.0.0",
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/environments/"
    ],
    "testRunner": "jest-jasmine2"
  },
  "scripts": {
    "ng": "ng",
    "prestart": "ts-appversion",
    "prebuild": "ts-appversion",
    "pretest": "ts-appversion",
    "start": "ng serve --proxy-config=proxy.conf.json",
    "start-dev": "npm run start -- --configuration local",
    "start-demo": "npm run start -- --configuration demo",
    "start-prod": "npm run start -- --configuration production",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "ng build",
    "build-prod": "npm run build -- --output-hashing=all --configuration production",
    "build-demo": "npm run build -- --output-hashing=all --configuration demo",
    "build-docker": "npm run build -- --output-hashing=all --configuration production --configuration docker --base-href=/",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stats": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.11",
    "@angular/cdk": "^12.2.11",
    "@angular/common": "^12.2.11",
    "@angular/compiler": "^12.2.11",
    "@angular/core": "^12.2.11",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "^12.2.11",
    "@angular/localize": "^12.2.11",
    "@angular/material": "^12.2.11",
    "@angular/material-moment-adapter": "^12.2.11",
    "@angular/platform-browser": "^12.2.11",
    "@angular/platform-browser-dynamic": "^12.2.11",
    "@angular/platform-server": "^12.2.11",
    "@angular/router": "^12.2.11",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "@saithodev/ts-appversion": "^2.2.0",
    "@stomp/ng2-stompjs": "^8.0.0",
    "@stomp/rx-stomp": "^1.1.4",
    "@types/file-saver": "^2.0.3",
    "angular-oauth2-oidc": "^12.1.0",
    "angular-oauth2-oidc-jwks": "^12.0.0",
    "angular-resize-event": "^3.0.2",
    "class-transformer": "^0.4.0",
    "core-js": "^3.19.0",
    "file-saver": "^2.0.5",
    "linq4js": "^2.2.3",
    "moment": "^2.29.4",
    "ngx-avatar": "^4.1.0",
    "ngx-logger": "^4.3.3",
    "ngx-mat-select-search": "^3.3.0",
    "ngx-material-file-input": "^2.1.1",
    "ngx-papaparse": "^5.0.0",
    "ngx-wow": "^2.0.1",
    "raw-loader": "^4.0.2",
    "rxjs": "^6.6.7",
    "sass": "^1.43.3",
    "tslib": "^2.0.0",
    "wowjs": "^1.1.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.11",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "^12.2.11",
    "@angular/compiler-cli": "^12.2.11",
    "@angular/language-service": "^12.2.11",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/jasmine": "^3.10.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-rxjs": "^3.3.7",
    "eslint-plugin-unicorn": "latest",
    "jest": "^27.3.1",
    "jest-preset-angular": "^10.0.1",
    "protractor": "~7.0.0",
    "rxjs-tslint-rules": "^4.34.8",
    "sw-precache": "^5.2.1",
    "ts-node": "^10.4.0",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
