    "eslint-plugin-rxjs": "~3.3.7",
    "eslint-plugin-unicorn": "~39.0.0",
    "jest": "^28.1.3",
    "jest-jasmine2": "^29.4.1",
    "jest-preset-angular": "^12.2.6",
    "ts-node": "~10.4.0",
    "typescript": "~4.6.4",
    "webpack-bundle-analyzer": "~4.5.0"
  }
}
